<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#9BD7E7"/>
            <solid
                android:color="#E8E0F0" />
            <size
                android:width="17dp"
                android:height="17dp" />
            <gradient
                android:startColor="#F0D4F0"
                android:endColor="#D0FCF0"
                android:type="linear" />
        </shape>
    </item>
</selector>